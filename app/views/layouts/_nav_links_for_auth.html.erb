<% if user_signed_in? %>
  <div class="inlineBlock link"><%= link_to 'Edit account', edit_user_registration_path %></div>
  <div class="inlineBlock link"><%= link_to 'Sign out', destroy_user_session_path, :method=>'delete' %></div>
<% else %>
  <div class="inlineBlock link"><%= link_to 'Sign in', new_user_session_path %></div>
  <div class="inlineBlock link"><%= link_to 'Sign up', new_user_registration_path %></div>
<% end %>

<% if user_signed_in? %>
  <% if current_user.try(:admin?) %>
    <div class="inlineBlock link"><%= link_to 'Users', users_path %></div>
  <% end %>
<% end %>
